public class DischargeStack {

    private static class Node {
        DischargeRecord record;
        Node next;

        Node(DischargeRecord record) {
            this.record = record;
            this.next = null;
        }
    }

    private Node top;
    private int size;

    public DischargeStack() {
        top = null;
        size = 0;
    }

    public void push(DischargeRecord record) {
        Node newNode = new Node(record);
        newNode.next = top;
        top = newNode;
        size++;
    }

    public DischargeRecord pop() {
        if (top == null) {
            return null;
        }
        DischargeRecord removed = top.record;
        top = top.next;
        size--;
        return removed;
    }

    public DischargeRecord peek() {
        if (top == null) {
            return null;
        }
        return top.record;
    }

    public int size() {
        return size;
    }

    public void printStack() {
        Node temp = top;
        while (temp != null) {
            System.out.println(temp.record);
            temp = temp.next;
        }
    }
}
