public class PatientList {              // create the patient list class
 private class Node {                   // create node class for linked list
        Patient patient;
        Node next;
        Node(Patient patient) {          // create node constructor
            this.patient = patient;
            this.next = null;
        }
    }
    private Node head;
    public PatientList() {
        head = null;                    // initialize list as empty
    }
    public void addPatient(Patient p) {  // add a new patient to the list
        Node newNode = new Node(p);
        if (head == null) {             // check if list is empty
            head = newNode;
            return;
        }
        Node temp = head;
        while (temp.next != null) {     // move to the end of the list
            temp = temp.next;
        }
        temp.next = newNode;
    }

    public void removePatient(int id) {   // remove patient by ID
        if (head == null) return;

        if (head.patient.id == id) {
            head = head.next;
            return;
        }

        Node current = head;
        while (current.next != null && current.next.patient.id != id) {      // search for patient to remove
            current = current.next;
        }

        if (current.next != null) {
            current.next = current.next.next;
        }
    }

    public Patient findPatient(int id) {
        Node temp = head;
        while (temp != null) {               // traverse the list
            if (temp.patient.id == id) {
                return temp.patient;
            }
            temp = temp.next;
        }
        return null;
    }

    public void printList() {               // print all patients in the list
        Node temp = head;
        while (temp != null) {
            System.out.println(temp.patient);
            temp = temp.next;
        }
    }
}
