public class DischargeRecord {                    // create the class
    public int patientId;
    public long dischargeTime;
 public DischargeRecord(int patientId) {        // create constructor

                                                 // create discharge record with patient ID and current time
        this.patientId = patientId;
        this.dischargeTime = System.currentTimeMillis();
 }
 public String toString() {                      // returns the patient ID and discharge time as a single string.
        return "Patient ID: " + patientId +
               ", Discharge Time: " + dischargeTime;
  }
}
