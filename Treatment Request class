public class TreatmentRequest {     // create TreatmentRequest class
    int patientId;
    long arrivalTime;
    boolean isPriority;
 public TreatmentRequest(int patientId, boolean isPriority){  // create a constructor
        this.patientId = patientId;
        this.isPriority = isPriority;
        this.arrivalTime = System.currentTimeMillis();
 }
}
